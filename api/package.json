{
  "name": "pawfect",
  "version": "0.0.1",
  "description": "",
  "author": "PULS software",
  "private": true,
  "license": "GL",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node --max_old_space_size=4096 ./node_modules/@nestjs/cli/bin/nest.js build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm migration:run",
    "drop": "npm run typeorm schema:drop",
    "apidoc": "npm run apidoc:auth && npm run apidoc:admin && npm run apidoc:customer && npm run apidoc:employee",
    "apidoc:auth": "apidoc -i src/modules/auth -o apidoc/auth -f '.apidoc.js$' -f '.apidoc.ts$'",
    "apidoc:admin": "apidoc -i src/modules/admin -o apidoc/admin -f '.apidoc.js$' -f '.apidoc.ts$'",
    "apidoc:customer": "apidoc -i src/modules/customer -o apidoc/customer -f '.apidoc.js$' -f '.apidoc.ts$'",
    "apidoc:employee": "apidoc -i src/modules/employee -o apidoc/employee -f '.apidoc.js$' -f '.apidoc.ts$'"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.5",
    "@nestjs/event-emitter": "0.0.3",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/platform-socket.io": "^6.11.11",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^8.4.5",
    "aws-sdk": "^2.825.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.0",
    "compression": "^1.7.4",
    "decimal.js": "^10.2.1",
    "fcm-node": "^1.6.1",
    "geojson": "^0.5.0",
    "luxon": "^1.25.0",
    "moment": "^2.29.3",
    "mustache": "^4.1.0",
    "nestjs-redis": "^1.3.3",
    "nestjs-typeorm-paginate": "^2.2.6",
    "nodemailer": "^6.4.17",
    "nodemailer-html-to-text": "^3.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sharp": "^0.27.0",
    "stripe": "^8.200.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.6",
    "@nestjs/testing": "^7.6.5",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.10",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "26.0.20",
    "@types/luxon": "^1.25.0",
    "@types/multer": "^1.4.5",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.20",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.28",
    "@types/sharp": "^0.27.1",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "apidoc": "^0.26.0",
    "axios": "^0.21.4",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".spec.ts$",
      ".e2e-spec.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
